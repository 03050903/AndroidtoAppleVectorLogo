<?xml version="1.0" encoding="utf-8"?>
    <vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:viewportWidth="170"
    android:viewportHeight="170"
    android:width="170dp"
    android:height="170dp">
    <path
        android:name="head"
        android:fillColor="@color/android_green"
        android:pathData="@string/android_head"/>
    <path android:name="left eye"
        android:fillColor="#FFFFFF"
        android:pathData="@string/left_eye_open"/>

    <path android:name="right eye"
        android:fillColor="#FFFFFF"
        android:pathData="@string/right_eye_open"/>

    <path android:name="left antenna"
        android:strokeColor="@color/android_green"
        android:strokeWidth="1"
        android:pathData="@string/left_antenna_normal"/>
    <path android:name="right antenna"
        android:strokeColor="@color/android_green"
        android:strokeWidth="1"
        android:pathData="@string/right_antenna_normal"/>

</vector>